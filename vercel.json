{
  "version": 2,
  "routes": [
    { "src": "/(.*)", "dest": "/$1" }
  ],
  "buildCommand": "cd apps/frontend && pnpm install --no-frozen-lockfile && pnpm run build",
  "installCommand": "npm install -g pnpm@9.15.9 && pnpm install --no-frozen-lockfile",
  "regions": ["iad1"],
  "env": {
    "PNPM_VERSION": "9.15.9"
  }
}