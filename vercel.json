{
  "version": 2,
  "routes": [
    { "src": "/(.*)", "dest": "/$1" }
  ],
  "buildCommand": "npm install -g pnpm@9.15.9 && pnpm install --no-frozen-lockfile && pnpm run -r build",
  "installCommand": "npm install -g pnpm@9.15.9 && pnpm install --no-frozen-lockfile",
  "regions": ["iad1"],
  "env": {
    "NODE_VERSION": "18.17.1",
    "NPM_CONFIG_LEGACY_PEER_DEPS": "true"
  }
}